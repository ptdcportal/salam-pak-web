{
  "name": "ptdc-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "starts": "npm run env -s && ng serve --disableHostCheck --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate",
    "bundle-report:prod": "ng build --stats-json --prod && webpack-bundle-analyzer dist/stats-es2015.json"
  },
  "dependencies": {
    "@agm/core": "^3.0.0-beta.0",
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/google-maps": "^10.2.4",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-free": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "angular2-text-mask": "^9.0.0",
    "angularx-social-login": "^3.4.1",
    "bootstrap": "^4.1.1",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "ngx-bootstrap": "^6.1.0",
    "ngx-pagination": "^5.0.0",
    "ngx-sharebuttons": "^8.0.1",
    "ngx-slick-carousel": "^0.5.1",
    "ngx-smart-modal": "^7.4.1",
    "ngx-toastr": "^13.0.0",
    "path": "^0.12.7",
    "rxjs": "^6.5.5",
    "slick-carousel": "^1.8.1",
    "tslib": "^1.10.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/googlemaps": "^3.39.12",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^25.1.2",
    "@types/jquery": "^3.5.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "gulp": "^4.0.2",
    "gulp-ext-replace": "^0.3.0",
    "gulp-gzip": "^1.4.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-minify": "^3.1.0",
    "gulp-purifycss": "^0.2.0",
    "hads": "^2.0.1",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.3",
    "imagemin-webp": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "stylelint": "~13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.16.0",
    "ts-node": "^8.8.1",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^4.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
